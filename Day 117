class Solution {
    int countWays(int n) {
        // your code here
        if(n == 1) return 1;
        if(n == 2) return 2;
        
        int a = 1, b = 2, c = 0;
        
        for(int i = 3; i <= n; i++){
            c = a + b;
            a = b;
            b = c;
        }
        return c;
    }
}
